[run]
source = src/discord_integration
omit =
    tests/*
    */__pycache__/*
    */migrations/*
    */venv/*
    */.venv/*

[report]
show_missing = True
skip_covered = False
precision = 2
fail_under = 90

[html]
directory = logs/htmlcov-discord
title = Discord Integration Service Coverage Report

[xml]
output = logs/coverage-discord.xml
