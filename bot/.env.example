# Discord Bot Configuration - Example Environment File
# Copy this file to .env and fill in your actual values

# Bot Authentication
DISCORD_BOT_TOKEN=YOUR_BOT_TOKEN_HERE
DISCORD_CLIENT_ID=YOUR_CLIENT_ID_HERE

# Environment-specific Discord Server
DISCORD_GUILD_ID=YOUR_GUILD_ID_HERE
DISCORD_GUILD_IDS=YOUR_GUILD_ID_HERE
ENVIRONMENT=dev

# Webhook Configuration
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_URL_HERE

# API Integration
API_BASE_URL=http://localhost:8001
BOT_API_URL=http://localhost:8002
FRONTEND_URL=http://localhost:8081

# Bot Settings
BOT_JWT=${BOT_JWT:-}
BOT_PREFIX=!
LOG_LEVEL=info

# Integration Flags (Enable testing mode)
DISCORD_BOT_READY=true
LIVE_TRIGGERS_ENABLED=false
CODEX_DRY_RUN=true

# Multi-environment Support
DISCORD_DEV_GUILD_ID=YOUR_DEV_GUILD_ID_HERE
DISCORD_PROD_GUILD_ID=YOUR_PROD_GUILD_ID_HERE
