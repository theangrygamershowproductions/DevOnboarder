# CI environment configuration for GitHub Actions
# Sanitized values for automated testing

# Database (SQLite for CI tests)
DATABASE_URL=sqlite:///./test_devonboarder.db

# Discord Bot (test/mock token for CI)
DISCORD_TOKEN=ci_test_token_placeholder

# JWT Secret (test secret for CI)
JWT_SECRET=ci_test_jwt_secret_for_automated_testing

# Environment flags
IS_ALPHA_USER=false
IS_FOUNDER=false

# Service ports
BACKEND_PORT=8001
BOT_PORT=8002
FRONTEND_PORT=8081
AUTH_PORT=8002

# Development flags
DEBUG=true

# CI-specific flags
CI=true
NODE_ENV=test
PYTHON_ENV=test
